    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug cli",
            "type": "debugpy",
            "request": "launch",
            "module": "elo_lib",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}",
            "args": [
                "getseason",
                "fad050beee834db88fa9f2eb28ce5a5c"
            ]
        },
    ]
}